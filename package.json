{
  "name": "connect-concierge",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start": "npm run ng-config-dev && ng serve --proxy-config proxy.config.json",
    "build": "ng build --prod",
    "test": "ng test",
    "build-war-dev": "npm run ng-config-dev && ng build --base-href /connectconcierge/ && gulp build:war:properties",
    "build-war-prod": "npm run ng-config-prod && ng build --prod --base-href /connectconcierge/ && gulp build:war:properties",
    "build-artifactory": "npm run ng-config-prod && ng build --prod --base-href /connectconcierge/ && gulp build:artifactory",
    "deploy-war-dev": "npm run ng-config-dev && ng build && gulp build:war:deploy",
    "deploy-war-prod": "npm run ng-config-prod && ng build --prod --base-href /connectconcierge/ && gulp build:war:deploy",
    "deploy-lang": "gulp deploy:lang",
    "deploy-artifactory": "npm run ng-config-prod && ng build --prod --base-href /connectconcierge/ && gulp build:artifactory:deploy",
    "ng-config-dev": "node build-configuration/build-config.js dev",
    "ng-config-prod": "node build-configuration/build-config.js prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.7",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@ng-bootstrap/ng-bootstrap": "^2.2.0",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "animate.css": "^3.6.1",
    "bootstrap": "^4.1.1",
    "cometd": "^3.1.4",
    "core-js": "^2.5.4",
    "ngx-toastr": "^8.8.0",
    "rxjs": "^6.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "gulp": "^3.9.1",
    "gulp-sftp": "^0.1.5",
    "gulp-sync": "^0.1.4",
    "gulp-zip": "^4.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-cmd": "^3.0.0",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
